{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "A repository to try Vue.js SSR usages.",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "corss-env NODE_ENV=production node server",
    "build": "rimraf dist && node run build:client && npm run build:server",
    "build:client": "corss-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "corss-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "keywords": [
    "Vue",
    "SSR",
    "Express",
    "Node.js"
  ],
  "author": "Meowu",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "express": "^4.16.2",
    "querystring": "^0.2.0",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.9",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.9",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
